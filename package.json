{
  "name": "my-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && npm run optimize",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build && npm run optimize",
    "optimize": "node scripts/build-optimize.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "security-check": "npm audit --audit-level moderate",
    "test:build": "npm run build && npm run start",
    "clean": "rm -rf .next out node_modules/.cache",
    "precommit": "npm run lint && npm run type-check"
  },
  "dependencies": {
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "critters": "^0.0.23",
    "dompurify": "^3.2.2",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-intersection-observer": "^9.14.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/validator": "^13.12.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
